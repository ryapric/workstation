#!/usr/bin/env zsh

# Shared shell rc file
source "${HOME}/.sharedrc"

### ZSH stuff
export ZSH="${HOME}/.oh-my-zsh"
ZSH_THEME="gnzh"
plugins=(
  docker
  docker-compose
  git
  python
  ubuntu
)

source "${ZSH}/oh-my-zsh.sh"

### mise-en-place
eval "$(mise activate zsh)"

# shellcheck disable=SC1091
source "${HOME}/.sharedrc-final"
